public class Stars
{
    public static void main (String[]args)
    {
        
        final int MAX_ROWS = 10;
        
        for(int limit = 1; limit <= MAX_ROWS; limit++)
        {
            for(int star = 10; star >= limit; star--)
            {
                System.out.print("*");
            }//End inner for loop
            
            System.out.println();
            
        }//End outer for loop
        
       
    }//End method main
}//End class Stars
